{
  "name": "grpc-node-ts",
  "description": "gRPC Node.JS server, in TypeScript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc --skipLibCheck",
    "prod": "npx tsc --skipLibCheck && node ./dist/server/server.js",
    "dev": "ts-node src/server/server.ts",
    "watch": "ts-node src/server/server.ts",
    "proto-gen": "./scripts/protoc.sh"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/node": "^14.0.23",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "typescript": "^3.9.6"
  }
}
